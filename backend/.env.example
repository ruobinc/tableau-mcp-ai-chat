# AWS Bedrock Configuration
AWS_REGION=ap-northeast-1
AWS_ACCESS_KEY=your_aws_access_key_here
AWS_SECRET_KEY=your_aws_secret_key_here
AWS_SESSION_TOKEN=your_aws_session_token_here

# Bedrock Model Configuration
BEDROCK_MODEL_ID=apac.anthropic.claude-sonnet-4-20250514-v1:0
MAX_TOKENS=10000

# Tableau MCP Server Configuration
SERVER_SCRIPT_PATH=/path/to/your/tableau-mcp/build/index.js
TABLEAU_SERVER=https://prod-apnortheast-a.online.tableau.com
TABLEAU_SITE_NAME=your_site_name
TABLEAU_PAT_NAME=your_pat_name
TABLEAU_PAT_VALUE=your_pat_value

# Logging
LOG_LEVEL=debug